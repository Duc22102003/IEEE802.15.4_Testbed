# Tool invocations
Z3Light-MG24-KIT.axf: $(OBJS) $(USER_OBJS) makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU ARM C Linker'
	arm-none-eabi-gcc '@Z3Light-MG24-KIT.axf.rsp' -o Z3Light-MG24-KIT.axf 
	@echo 'Finished building target: $@'
	@echo ' '

	@echo 'Building out file: Z3Light-MG24-KIT.out'
	arm-none-eabi-objcopy "Z3Light-MG24-KIT.axf" "Z3Light-MG24-KIT.out"
	@echo ' '

	@echo 'Building hex file: Z3Light-MG24-KIT.hex'
	arm-none-eabi-objcopy -O ihex "Z3Light-MG24-KIT.axf" "Z3Light-MG24-KIT.hex"
	@echo ' '

	@echo 'Building bin file: Z3Light-MG24-KIT.bin'
	arm-none-eabi-objcopy -O binary "Z3Light-MG24-KIT.axf" "Z3Light-MG24-KIT.bin"
	@echo ' '

	@echo 'Building s37 file: Z3Light-MG24-KIT.s37'
	arm-none-eabi-objcopy -O srec "Z3Light-MG24-KIT.axf" "Z3Light-MG24-KIT.s37"
	@echo ' '

	@echo 'Running size tool'
	arm-none-eabi-size "Z3Light-MG24-KIT.axf" -A
	@echo ' '

